{
  "name": "Googly Eyes",
  "description": "Hack & Roll 2022",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
		"scripts": ["narrate.js", "background.js"]
  },
  "permissions": [
    "storage", 
    "activeTab", 
    "contextMenus", 
    "tts",
    "http://*/",
		"https://*/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}