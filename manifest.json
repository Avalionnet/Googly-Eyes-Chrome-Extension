{
  "name": "HNR2022",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "css":["changeDisplay.css"],
      "js":["changeDisplay.js"]
    }
  ],
  "background": {
    "background": "background.js"
  },
  "permissions": ["storage"]
}