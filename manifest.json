{
  "name": "Googly Eyes",
  "description": "Hack & Roll 2022",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "js":["contentScript/darkMode.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "/PopUp/popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  }

}